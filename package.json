{
  "name": "lynx-js-snippets",
  "displayName": "Lynxjs Snippets .tsx",
  "description": "Available now: VSCode • Cursor • Windsurf • Trae.ai • Kiro & (Firebase Studio)... 雨",
  "version": "0.6.0",
  "publisher": "bastndev",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Snippets"],
  "keywords": [
    "Lynxjs",
    "snippets",
    "productivity",
    "lynxjs snippet",
    "typescript",
    "reactLynx",
    "react",
    "Lynx"
  ],
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#0a0a0a",
    "theme": "dark"
  },
  "homepage": "https://github.com/bastndev/Lynx-js-Snippets#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/bastndev/Lynx-js-Snippets"
  },
  "bugs": {
    "url": "https://github.com/bastndev/Lynx-js-Snippets/issues"
  },
  "author": {
    "name": "Gohit Bastian",
    "email": "gobasdev@gmail.com",
    "url": "https://www.bastndev.com"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/bastndev"
  },

  "activationEvents": [
    "onLanguage:typescriptreact",
    "onLanguage:javascriptreact"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lynx-js-snippets.manage",
        "title": "Manage Snippets",
        "category": "LynxJS"
      },
      {
        "command": "lynx-js-snippets.createSnippet",
        "title": "Create New Snippet",
        "category": "LynxJS"
      },
      {
        "command": "lynx-js-snippets.refreshSnippets",
        "title": "Refresh Snippets",
        "category": "LynxJS"
      }
    ],
    "snippets": [
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  }
}
